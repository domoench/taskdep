body {
  font: 14px "Century Gothic", Futura, sans-serif;
  margin: 20px;
}

.graphviz {
  border: 1px solid gray;
}

.links line {
  stroke: #999;
  stroke-opacity: 0.6;
  stroke-width: 4px;
}

.links line.selected {
  stroke-opacity: 1.0;
	stroke-dasharray: 10,2;
  stroke-width: 5px;
}

marker#arrow path {
	fill: #999;
}
